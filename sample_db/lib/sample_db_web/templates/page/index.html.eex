<%
result = Db.query( "select * from members" )
data = result |> Db.columns_rows
%>
<table border="1">
<%= for record <- data do %>
<tr>
    <td><%= record[ "name" ] %></td>
    <td><%= record[ "age" ] %></td>
    <td><%= record[ "team" ] %></td>
    <td><%= record[ "position" ] %></td>
</tr>
<% end %>
</table>
